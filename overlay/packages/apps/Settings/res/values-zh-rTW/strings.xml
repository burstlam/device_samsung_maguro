    <string name="auto_brightness_split_lux_format">新增水平 (於 <xliff:g id="min">%1$d</xliff:g> 及 <xliff:g id="max">%2$d</xliff:g> lux 之間)</string>
    <string name="auto_brightness_level_options">亮度水平</string>
    <string name="auto_brightness_menu_edit_lux">編輯光線感應器取值</string>
